<div class="col-md-7">
  <p id="notice"><%= notice %></p>
  <h1>Listing Friends Joined</h1>
<table class="table table-bordered table-condensed table-hover">
    <thead>
      <tr>
        <th>id</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @joins.each do |joined| %>
        <tr>
          <td><%= joined.id %></td>
          <td><%= link_to "delete", joins_path(joined), method: 'delete',data: { confirm: 'Are you sure you want to delete?', :class => 'delete_order' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
